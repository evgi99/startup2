{
  "name": "startup2",
  "version": "1.0.0",
  "description": "A project to hold basic file and configuration to start devlopment new project  with node and react",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:clean build:build build:copy-files",
    "build:clean": "rimraf dist",
    "build:build": "node_modules/.bin/bable src -d dist --source-maps",
    "build:copy-files": "cp README.md package.json .npmignore dist",
    "dev": "npm-run-all -p dev:clean dev:build-watch dev:run-server",
    "dev:clean": "rimraf main/dist",
    "dev:build": "node_modules/.bin/webpack",
    "dev:build-watch": "node_modules/.bin/webpack -w",
    "dev:run-server": "node main/server.js",
    "test": "jest",
    "test:watch": "./node_mudules/.bin/jest watchAll"
  },
  "author": "Evgi",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/evgi99/startup2.git"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "jest": "^21.2.1",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.0"
  }
}
